@inproceedings{cattuto_time-varying_2013,
	location = {New York New York},
	title = {Time-varying social networks in a graph database: a Neo4j use case},
	isbn = {978-1-4503-2188-4},
	url = {https://dl.acm.org/doi/10.1145/2484425.2484442},
	doi = {10.1145/2484425.2484442},
	shorttitle = {Time-varying social networks in a graph database},
	abstract = {Representing and eﬃciently querying time-varying social network data is a central challenge that needs to be addressed in order to support a variety of emerging applications that leverage high-resolution records of human activities and interactions from mobile devices and wearable sensors. In order to support the needs of speciﬁc applications, as well as general tasks related to data curation, cleaning, linking, post-processing, and data analysis, data models and data stores are needed that aﬀord eﬃcient and scalable querying of the data. In particular, it is important to design solutions that allow rich queries that simultaneously involve the topology of the social network, temporal information on the presence and interactions of individual nodes, and node metadata. Here we introduce a data model for time-varying social network data that can be represented as a property graph in the Neo4j graph database. We use time-varying social network data collected by using wearable sensors and study the performance of real-world queries, pointing to strengths, weaknesses and challenges of the proposed approach.},
	eventtitle = {{SIGMOD}/{PODS}'13: International Conference on Management of Data},
	pages = {1--6},
	booktitle = {First International Workshop on Graph Data Management Experiences and Systems},
	publisher = {{ACM}},
	author = {Cattuto, Ciro and Quaggiotto, Marco and Panisson, André and Averbuch, Alex},
	urldate = {2024-11-11},
	date = {2013-06-23},
	langid = {english},
	file = {Cattuto et al. - 2013 - Time-varying social networks in a graph database .pdf:C\:\\Users\\neuro\\Zotero\\storage\\46NFG8NY\\Cattuto et al. - 2013 - Time-varying social networks in a graph database .pdf:application/pdf},
