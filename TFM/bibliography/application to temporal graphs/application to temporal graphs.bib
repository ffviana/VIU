	pages = {43--48},
	booktitle = {2016 17th {IEEE} International Conference on Mobile Data Management ({MDM})},
	publisher = {{IEEE}},
	author = {Pereira, Fabiola S.F. and De Amo, Sandra and Gama, Joao},
	urldate = {2024-11-11},
	date = {2016-06},
	langid = {english},
	file = {Pereira et al. - 2016 - Evolving Centralities in Temporal Graphs A Twitte.pdf:C\:\\Users\\neuro\\Zotero\\storage\\WHVAGMUE\\Pereira et al. - 2016 - Evolving Centralities in Temporal Graphs A Twitte.pdf:application/pdf},
